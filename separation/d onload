async onload() {
    console.log('Loading Zoro Plugin');
    
    // Load settings first
    await this.loadSettings();

  // Inject CSS

this.injectCSS();

    
    // Register code block processor
    this.registerMarkdownCodeBlockProcessor('zoro', this.processZoroCodeBlock.bind(this));
    
    // Register new search code block processors
    this.registerMarkdownCodeBlockProcessor('zoro-search', this.processZoroSearchCodeBlock.bind(this));
    
    // Register inline link processor
    this.registerMarkdownPostProcessor(this.processInlineLinks.bind(this));
    
    // Add plugin settings
    this.addSettingTab(new ZoroSettingTab(this.app, this));
  }
